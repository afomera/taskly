<div class="pull-sm-right">
  <%= link_to "New Task List", root_path, class: 'btn btn-secondary' %>
</div>
<h1>Your tasks</h1>

<div class="col-sm-4">
  <div class="card">
    <div class="card-block">
      <h2>Tasklist name</h2>
      <ul>
        <% @tasks.each do |task| %>
          <li><h4><%= task.name %></h4></li>
        <% end %>
      </ul>
    </div> <!-- ./card-block -->
  </div> <!-- ./card -->
</div>
